{
    "extends": ["prettier", "airbnb-base"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "env": {
      "commonjs": true,
      "node": true
    },
    "rules": {
      "no-console": 0,
      "indent": 0,
      "linebreak-style": 0,
      "operator-linebreak": 0,
      "comma-dangle": 0,
      "no-useless-catch": 0,
      "max-len": 0,
      "default-case": 0,
      "prettier/prettier":0,/* [
       0,
        {
          "singleQuote": true,
          "parser": "flow",
          "printWidth": 120,
          "tabWidth": 4,
          "semi": true
        }
      ]
    },*/
    "plugins": ["prettier"]
  }
}